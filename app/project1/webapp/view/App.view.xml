<mvc:View
	displayBlock="true"
	height="100%"
	xmlns="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<FlexibleColumnLayout id="app" backgroundDesign="Solid" stateChange="onStateChanged" layout="ThreeColumnsMidExpanded">
		<beginColumnPages>
			<mvc:XMLView id="beginView" viewName="app.project1.view.List"/>
		</beginColumnPages>
        <midColumnPages>
			<mvc:XMLView id="detailView" viewName="app.project1.view.Fields"/>
		</midColumnPages>
        <endColumnPages>
        <mvc:XMLView id="thirdView" viewName="app.project1.view.Association"/>
    </endColumnPages>
	</FlexibleColumnLayout>
    
</mvc:View>